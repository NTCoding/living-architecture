{
  "name": "@living-architecture/riviere-cli",
  "version": "0.7.7",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@living-architecture/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "bin": {
    "riviere": "./dist/bin.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@living-architecture/riviere-builder": "workspace:*",
    "@living-architecture/riviere-extract-config": "workspace:*",
    "@living-architecture/riviere-extract-ts": "workspace:*",
    "@living-architecture/riviere-query": "workspace:*",
    "@living-architecture/riviere-schema": "workspace:*",
    "commander": "^14.0.2",
    "glob": "^11.0.2",
    "ts-morph": "^24.0.0",
    "yaml": "^2.7.0"
  }
}
