{
  "name": "@living-architecture/docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "outputs": ["{projectRoot}/.vitepress/dist"],
        "options": {
          "command": "vitepress build",
          "cwd": "apps/docs"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "continuous": true,
        "options": {
          "command": "vitepress dev",
          "cwd": "apps/docs"
        }
      },
      "preview": {
        "executor": "nx:run-commands",
        "dependsOn": ["build"],
        "options": {
          "command": "vitepress preview",
          "cwd": "apps/docs"
        }
      }
    }
  },
  "dependencies": {
    "vitepress": "^1.5.0"
  }
}
